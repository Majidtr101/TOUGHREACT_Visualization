frames(41) = getframe(figure41);

F(41) = struct('cdata',[],'colormap',[]); % Replace the number inside F(41) with the number of figures you have 
for i = 1:41 % make sure the maximum number is equal to your last figure number
    F(i) = im2frame(frames(i).cdata);
end

writerObj = VideoWriter('Dolomite_bigpic.avi');
writerObj.FrameRate = .75; % you can change how many seconds each graph will show
open(writerObj);
for i = 1:41 % make sure the maximum number is equal to your last figure number
    writeVideo(writerObj,F(i));
end
close(writerObj);
